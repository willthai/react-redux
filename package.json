{
  "name": "ventura",
  "version": "1.0.0",
  "description": "Curse React Test",
  "main": "index.html",
  "scripts": {
    "start": "node --expose-gc --max_old_space_size=4096 ./node_modules/gulp/bin/gulp",
    "test": "jest"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "__tests__/utils"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js"
  },
  "author": "",
  "license": "ISC",
  "private": true,
  "dependencies": {
    "lodash": "^4.17.2",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "whatwg-fetch": "^2.0.1"
  },
  "devDependencies": {
    "@types/jest": "^16.0.0",
    "@types/lodash": "^4.14.39",
    "@types/react": "^0.14.50",
    "@types/react-dom": "^0.14.19",
    "@types/react-redux": "^4.4.33",
    "@types/react-router": "^2.0.39",
    "@types/redux": "^3.6.31",
    "@types/redux-thunk": "^2.1.31",
    "@types/whatwg-fetch": "^0.0.32",
    "browser-sync": "^2.15.0",
    "connect-history-api-fallback": "^1.3.0",
    "gulp": "^3.9.1",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^1.9.1",
    "gulp-util": "^3.0.7",
    "jest": "^17.0.3",
    "react-test-renderer": "^15.4.0",
    "ts-jest": "^17.0.0",
    "ts-loader": "^1.2.2",
    "tslint": "^4.0.1",
    "typescript": "^2.0.10",
    "webpack": "^1.13.3"
  }
}
